#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules/he/bin/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules/he/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/node_modules:/Users/tanoaleu/workspace/node_modules:/Users/tanoaleu/node_modules:/Users/node_modules:/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules/he/bin/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules/he/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/node_modules:/Users/tanoaleu/workspace/node_modules:/Users/tanoaleu/node_modules:/Users/node_modules:/node_modules:/Users/tanoaleu/workspace/SocialMediaApp/fullstack/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../he/bin/he" "$@"
else
  exec node  "$basedir/../he/bin/he" "$@"
fi
